<% form_for(@organization) do |f| %>
  <%= f.error_messages %>
  <fieldset><legend>Organization:</legend>
      <%= f.label :name %>: <%= f.text_field :name %><br />
      <%= f.label :dba %>: <%= f.text_field :dba %><br />
      <%= f.label :department %>: <%= f.text_field :department %><br />
      <%= f.label :tax_id_number %>: <%= f.text_field :tax_id_number %><br />
      <%= f.submit 'Create' %>
  </fieldset>
<% end %>