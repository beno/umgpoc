<table>
  <tr>
    <th>Web Address</th>
    <th>Purpose</th>
  </tr>

<% @internet_addresses.each do |internet_address| %>
  <tr>
    <td><%=h internet_address.internet_address %></td>
    <td><%=h internet_address.purpose %></td>
    <td><%= link_to 'Show', polymorphic_url([internet_address.locatable, internet_address]) %></td>
    <td><%= link_to 'Edit', polymorphic_url([internet_address.locatable, internet_address], :action => 'edit') %></td>
    <td><%= link_to 'Destroy', polymorphic_url([internet_address.locatable, internet_address]), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New internet_address', polymorphic_url([@locatable, InternetAddress.new], :action => 'new') %> | <%= link_to 'Back', url_for(@locatable) %>